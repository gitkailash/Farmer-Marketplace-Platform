# API Configuration
VITE_API_URL=http://localhost:5000/api

# Environment
VITE_NODE_ENV=development

# Production Configuration (uncomment and configure for production)
# For AWS EC2 deployment with IP (no domain):
# VITE_API_URL=http://100.49.247.47/api
# For domain-based deployment:
# VITE_API_URL=https://api.your-production-domain.com/api
# VITE_NODE_ENV=production

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true

# Cache Configuration
VITE_CACHE_DURATION=300000

# Performance Configuration
VITE_LAZY_LOADING=true
VITE_IMAGE_OPTIMIZATION=true

# Security Configuration
VITE_CSP_ENABLED=false