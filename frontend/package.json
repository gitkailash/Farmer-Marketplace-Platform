{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "preview": "vite preview",
    "preview:prod": "vite preview --port 3000",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && npm run type-check",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/stats.html",
    "i18n:extract": "tsx scripts/translation-tools.ts",
    "i18n:validate": "tsx scripts/translation-tools.ts",
    "i18n:report": "tsx scripts/translation-tools.ts",
    "i18n:check": "node scripts/validate-translations.js",
    "translations:optimize": "node ../scripts/optimize-translations.js",
    "translations:deploy": "npm run translations:optimize && node ../scripts/cdn-config.js upload",
    "translations:deploy:aws": "CDN_PROVIDER=aws npm run translations:deploy",
    "translations:deploy:cloudflare": "CDN_PROVIDER=cloudflare npm run translations:deploy",
    "translations:deploy:azure": "CDN_PROVIDER=azure npm run translations:deploy",
    "translations:configure": "node ../scripts/cdn-config.js configure"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "@tiptap/extension-placeholder": "^3.15.3",
    "@tiptap/react": "^3.15.3",
    "@tiptap/starter-kit": "^3.15.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.13.2",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^1.0.4",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.47.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "glob": "^10.3.10",
    "jsdom": "^23.0.1",
    "msw": "^2.0.8",
    "postcss": "^8.5.6",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.1",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^1.0.4"
  }
}
