# Staging Environment Configuration
# Copy this file to .env for staging deployment

# Database Configuration
MONGO_ROOT_USERNAME=staging_user
MONGO_ROOT_PASSWORD=staging_password_change_me
MONGO_DB_NAME=farmer-marketplace-staging

# Backend Configuration
NODE_ENV=production
JWT_SECRET=staging-jwt-secret-change-this-to-32-chars-minimum
JWT_EXPIRES_IN=7d
BCRYPT_ROUNDS=12
CORS_ORIGIN=https://staging.your-domain.com
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=50
LOG_LEVEL=debug
ENABLE_RESPONSE_CACHE=false

# Frontend Configuration
VITE_API_URL=https://api-staging.your-domain.com/api
VITE_NODE_ENV=production

# Development Features (enabled in staging)
VITE_ENABLE_DEBUG=true
VITE_ENABLE_ANALYTICS=false